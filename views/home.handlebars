                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-info text-uppercase mb-2">Companhias</div>
                        <div class="h5 mb-0 font-weight-bold text-gray-800">{{companhiacont}}</div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-flag fa-2x text-gray-300"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

